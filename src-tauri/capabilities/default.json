{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "process:allow-restart",
    "process:allow-exit",
    "updater:allow-check",
    "updater:allow-download-and-install"
  ]
}
